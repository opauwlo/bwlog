<div class="card bg-dark">
  <div class="card-body px-4">
    {{#each posts}}
        <h1 class="card-title text-light">{{dataValues.titulo}}</h1>
        <small class="text-secondary">{{dataValues.updatedAt}}</small>
        <p class="card-text text-light">{{dataValues.conteudo}}</p>
        
        <hr class="text-light">
    {{/each}}
  </div>
</div>